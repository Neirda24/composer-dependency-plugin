{
    "name": "neirda24/composer-dependency-plugin",
    "description": "Plugin to define a composer parent. If a dependency is already installed in the parent project, it won't be loaded.",
    "keywords": ["composer", "parent dependency"],
    "type": "composer-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Adrien Schaegis",
            "email": "rochesadrien@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3",
        "composer-plugin-api": "^1.0",
        "neirda24/tools-bundle": "~1"
    },
    "require-dev": {
        "composer/composer": "~1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Neirda24\\Composer\\ParentDependencyPlugin\\": "lib/"
        }
    },
    "extra": {
        "class": "Neirda24\\Composer\\ParentDependencyPlugin\\Neirda24ParentDependencyPlugin"
    }
}
